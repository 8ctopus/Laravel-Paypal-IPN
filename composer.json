{
    "name": "8ctopus/laravel-paypal-ipn",
    "description": "Paypal IPN class for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "8ctopus",
            "email": "hello@octopuslabs.io"
        },
        {
            "name": "Fahim Shariar",
            "email": "thebdcoder@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.9",
        "phpmd/phpmd": "^2.13"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Oct8pus\\PaypalIPN\\": "src/"
        }
    },
    "scripts": {
        "fix": "php-cs-fixer fix --verbose",
        "fix-risky": "php-cs-fixer fix --allow-risky=yes --verbose",
        "phpstan": "phpstan analyse --level 9 src",
        "phpmd": "phpmd src ansi phpmd.xml"
    }
}
